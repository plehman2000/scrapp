        <script>
        jQuery(document).ready(function ($)
        {
            // Close form notice
            $(document).on('click', '.form-notice .span-icon, .modal-form-notice .span-icon', function (e)
            {
                e.preventDefault();
                e.stopPropagation();
                $(this).closest('.form-notice, .modal-form-notice').slideUp();
            });

            // Close form notice unverifiedaccount
            $(document).on('click', '.form-notice.unverified', function (e)
            {
                e.preventDefault();
                e.stopPropagation();
                $(this).slideUp();
            });

            // Horizontal Scrolling Nav
            $('.horizontal-scroll-nav').each(function ()
            {
                var $this = $(this);
                var $active = $this.find('a.active');
                $this.find('.horizontal-scroll-nav-content').scrollLeft($active.offset().left - 10);
            });

            jQuery.fn.isInViewport = function ()
            {
                var elementTop = jQuery(this).offset().top;
                var elementBottom = elementTop + jQuery(this).outerHeight();

                var viewportTop = jQuery(window).scrollTop();
                var viewportBottom = viewportTop + jQuery(window).height();

                return elementBottom > viewportTop && elementTop < viewportBottom;
            };

            jQuery(window).on('resize scroll', function ()
            {
                jQuery('.lazy').not('img').each(function ()
                {
                    if (jQuery(this).isInViewport() && jQuery(this).hasClass("lazy"))
                    {
                        jQuery(this).removeClass('lazy');
                        jQuery(this).addClass('fade-in-image');
                    }
                });

                jQuery('img.object-fit-cover').each(function ()
                {
                    if (jQuery(this).isInViewport())
                    {
                        var Urlimg = jQuery(this).data('lazy');
                        jQuery(this).attr('src', Urlimg);

                    }
                });

                jQuery('img.rounded-circle.lazy').each(function ()
                {
                    if (jQuery(this).isInViewport())
                    {
                        var Urlimg = jQuery(this).data('mylazy');
                        jQuery(this).attr('src', Urlimg);
                    }
                });
            });

            jQuery(function ()
            {
                jQuery('img.object-fit-cover').each(function ()
                {
                    if (jQuery(this).isInViewport())
                    {
                        var Urlimg = jQuery(this).data('lazy');
                        jQuery(this).attr('src', Urlimg);
                    }
                });
            });
        });
    </script>
            <script>
        (function($){
            var $modalNotifications = $('#modal-notifications');
            var $footerMobileNav = $('.portal__navbar-mobile.sitewide');

            $(window).on('resize', function ()
            {
                // Mobile
                if (window.innerWidth <= 768)
                {
                    // Open modal event
                    $modalNotifications.on('show.bs.modal', function(){
                        $modalNotifications.find('.modal-body').css('padding-bottom', '80px');
                        $footerMobileNav.css('z-index', 999999999);
                    });

                    // Close modal event
                    $modalNotifications.on('hide.bs.modal', function(){
                        $modalNotifications.find('.modal-body').css('padding-bottom', '');
                        $footerMobileNav.css('z-index', '');
                    });

                    // Open
                    if ($modalNotifications.hasClass('show'))
                    {
                        $modalNotifications.find('.modal-body').css('padding-bottom', '80px');
                        $footerMobileNav.css('z-index', 999999999);
                    }
                    else
                    {
                        $modalNotifications.find('.modal-body').css('padding-bottom', '');
                        $footerMobileNav.css('z-index', '');
                    }
                }
                else
                {
                    $modalNotifications.find('.modal-body').css('padding-bottom', '');
                    $footerMobileNav.css('z-index', '');
                }
            }).trigger('resize');
        })(jQuery);
    </script>
