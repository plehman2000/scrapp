<script type="text/javascript">
    jQuery(function($)
    {
        let scrollTop = 0;
        const footerContainer = $('.portal__navbar-mobile.sitewide');
        var $mainMobileNav = $('#menu-mobile-fullscreen');
        
        $('.trigger-search').on('click', function (e)
        {
            e.preventDefault();
            
            $(document.body).addClass('is-modal-search');
            $('.modal-search-inner form input[name="q"]').focus();
        });
        
        $(document).on('scroll', function ()
        {
            let st = $(this).scrollTop();
            let maxScroll = $(window).scrollTop() + $(window).height() + 60;
            let documentScroll = $(document).height();
            
            if (scrollTop < 20)
            {
                scrollTop = st;
                return;
            }

            if (st < scrollTop || maxScroll >= documentScroll)
            {
                // Show on scroll up
                footerContainer
                .removeClass('d-block')
                .removeClass('d-none')
                .addClass('d-block');
            }
            else
            {
                if (!$mainMobileNav.hasClass('show'))
                {
                    // Hide if scroll down
                    footerContainer
                    .removeClass('d-block')
                    .removeClass('d-none');
                }
            }

            // Update last position
            scrollTop = st;
        });
    });
</script>

