 jQuery('._t_filters').fadeTo(200, 1);

 function formatState (state) {
 if (!state.id) {
 return state.text;
 }

 var img = '';
 for(var i = 1; i <= 5; i++) {
 if(i <= parseInt(state.text))
 img += '<img src="data:image/png;base64,">';
 else
 img += '<img src="data:image/png;base64,">';
 }
 var $state = jQuery(img);

 return $state;
 };

