        },
        setPageVars: function() {
            this.setLinkTrackVars("eVar48,eVar129,eVar135,eVar136,eVar137,eVar138");
            s.eVar48 = this.getCNNTechStack();
            s.eVar122 = s.eVar123 = ""; // module load
