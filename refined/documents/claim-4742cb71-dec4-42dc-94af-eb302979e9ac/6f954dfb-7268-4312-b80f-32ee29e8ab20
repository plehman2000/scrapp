        getCNNUIEngagement: function() { //prop64 - UI Engagement
            var rval = (this.isNGTVPage() || this.isTVEPage()) ? "ngtv" : "cnn news";
            try { 
                if(typeof window.cnn_metadata.friendly_name != 'undefined') {
                    rval = window.cnn_metadata.friendly_name;
