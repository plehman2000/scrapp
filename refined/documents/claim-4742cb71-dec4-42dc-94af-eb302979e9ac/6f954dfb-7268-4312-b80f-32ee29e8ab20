        if (event !== "audio-progress") {
            setINSTAudioEvent(event, data);
            var currAudObj = data;
            currAudObj.playerid = playerid;
            trackMetrics({
                type: event,
                data: {
                    audio : currAudObj
                }
            });
        }
    } catch (e) {}
};

