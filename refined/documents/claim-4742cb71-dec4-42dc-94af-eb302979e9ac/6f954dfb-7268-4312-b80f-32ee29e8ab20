        getTemplateTypeLookup: function() {
            return {
                b: "blog",
                g: "game",
                it: "interactive",
                c: "content",
                in: "index",
                err: "error",
                e: "ecom",
                s: "signup",
                v: "video",
                sf: "section front",
                sr: "search results",
                fm: "forum",
                o: "other",
                ir: "ireport",
                sp: "specials",
                pm: "perfect market",
                bf: "blog front",
                bc: "blog category",
                t: "topic",
