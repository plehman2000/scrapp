<script>
            function domReady(fn) {
                document.addEventListener("DOMContentLoaded", fn);
                if (document.readyState === "interactive" || document.readyState === "complete" ) {
                    fn();
                }
            }
            domReady(function() {
                var readMore = document.querySelector('.article-body .read-more-button-link');
                if (readMore) {

                    function updateVisibleDynamicAdSlots() {
                        document.querySelectorAll('.responsive-ad .dynamic-ad-slot:not(.initialized)').forEach(function (slot) {
                            if (slot.offsetParent !== null) {
                                var parent = slot.closest('.responsive-ad');
                                var decodedHTML = new DOMParser().parseFromString(slot.dataset.src, 'text/html').body;

                                parent.innerHTML = '';
                                Array.from(decodedHTML.childNodes).forEach(function (node) {
                                    if (node.tagName === 'SCRIPT') {
                                        var script = document.createElement('script');
                                        if (node.src) {
                                            script.src = node.src;
                                            script.async = true;
                                        } else {
                                            script.textContent = node.textContent;
                                        }
                                        document.body.appendChild(script);
                                    } else {
                                        parent.appendChild(node);
                                    }
                                });
                                slot.classList.add('initialized');
                            }
                        });
                    }

                    var readMoreParent = readMore.closest('.read-more-button-wrap');
                    if (!readMoreParent.parentNode.classList.contains('body-main')) {
                        while (readMoreParent.parentNode && !readMoreParent.parentNode.classList.contains('body-main')) {
                            readMoreParent = readMoreParent.parentNode;
                            readMoreParent.insertAdjacentHTML('afterend', '<div class="read-more-button-after"></div>');
                        }
                    }

                    readMore.closest('.article-body').classList.add('read-more-visible');

                    // google ads hiiden fix
                    updateVisibleDynamicAdSlots();                  

                    readMore.addEventListener('click', function(e) {
                        e.preventDefault();
                        readMore.closest('.article-body').classList.remove('read-more-visible');
                        setTimeout(function() {
                            updateVisibleDynamicAdSlots();
                        }, 100);
                    })
                }
            })
        </script>
<script>
                document.addEventListener('DOMContentLoaded', function() {
                    var lastSentDepth = 0;
                    var step = 10;

                    function sendScrollEvent(depth) {
                       //  console.log('Sending scroll depth:', depth); // Debugging
                        window.dataLayer = window.dataLayer || [];
                        window.dataLayer.push({
                            event: 'article_scroll_depth',
                            scroll_depth: depth
                        });
                    }

                    function checkScrollDepth() {
                        var article = document.querySelector('body > .container.page-body');
                        if (!article) return;

                        var articleHeight = article.offsetHeight;
                        var viewportHeight = window.innerHeight;
                        var scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                        var scrollDepth = (scrollTop + viewportHeight - article.offsetTop) / articleHeight * 100;

                        var depth = Math.floor(scrollDepth/step)*step;

                        if (depth > 100) depth = 100;

                        if (depth > lastSentDepth) {
                            for (var i = lastSentDepth + step; i <= depth; i = i + step) {
                                sendScrollEvent(i);
                            }
                            lastSentDepth = depth;
                        }
                    }
                    window.addEventListener('scroll', checkScrollDepth);
                    checkScrollDepth();
                });
            </script>
