<script type="rocketlazyloadscript">
jQuery(document).ready(function() {
    // Function to check scroll position and fade out the '#ic_div_300x250_1' element
    function checkAndFade() {
        var totalHeight = jQuery(document).height();
        var viewportHeight = jQuery(window).height();

        var middlePosition = totalHeight / 2;
        var viewportMiddle = viewportHeight / 2;

        var scrollY = jQuery(window).scrollTop();

        // Check if the center of the viewport has reached the vertical middle of the entire HTML page
        if (scrollY + viewportMiddle >= middlePosition) {
            jQuery('#ic_div_300x250_1').fadeOut();
			jQuery('#ic_div_300x250_2').fadeIn();
        } else {
			jQuery('#ic_div_300x250_2').fadeOut();
            jQuery('#ic_div_300x250_1').fadeIn();
        }
    }

    jQuery(window).scroll(function() {
        checkAndFade();
    });

	jQuery('#ic_div_300x250_2').fadeOut();
    checkAndFade();
});

