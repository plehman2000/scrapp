// On scroll, reposition video player
// When the user scrolls the page, execute myFunction
window.onscroll = function () {
  videoScroll();
  railsScroll();
};

// Add the sticky class to the header when you reach its scroll position. 
//Remove "sticky" when you leave the scroll position
function videoScroll() {
if (jQuery('#video-widget').length) {
	  sticky = jQuery('#video-widget').offset();
	  if (window.pageYOffset > 300) {
      // header.classList.add("sticky");
      if (!(jQuery('#video-widget').hasClass('sticky'))) {
        jQuery('#video-widget').addClass('sticky').css('left', sticky.left);
        jQuery('#video-placeholder').addClass('appear');
      }
	  } else if (jQuery('#video-widget').hasClass('sticky')) {
      jQuery('#video-widget').removeClass('sticky');
      jQuery('#video-placeholder').removeClass('appear');
    }
  }
}
function railsScroll() {
  // Left and right page takeover rails
  winht = jQuery(window).height();
  pass_by = (jQuery('.tdc-header-wrap').outerHeight() + 1200) - winht;
  console.log(jQuery(window).scrollTop());
  if (jQuery(window).scrollTop() > pass_by) {
    console.log('should stay');
    jQuery('.stickyRails').css('position', 'fixed').css('bottom', '0').css('top', '')
      .css('height', '1200px');
  } else {
    jQuery('.stickyRails').css('position', 'absolute').css('bottom', '');
  }
}
jQuery(document).ready(() => {
  // Creates cookie for Google Optimize Vars
  const variant_cookie = readCookie('variant');
  if (!variant_cookie) { // if cookie does not exist yet, create cookie
    const vari = getParameterByName('vari');
    if (vari) { // vari has a value
      createCookie('variant', vari, 30);
    } else createCookie('variant', '1', 30);
  }
  // Attaches tracking to specific Newspaper blocks
  jQuery('.td_module_related_posts a').click(function () {
    link = $(this).attr('href');
    page = link.substr(link.lastIndexOf('/') + 1);
    ga('send', 'pageview', `/internal/rel-posts-footer/`);
  });
  jQuery('.td_block_1 a').click(function () {
    link = $(this).attr('href');
    page = link.substr(link.lastIndexOf('/') + 1);
    ga('send', 'pageview', `/internal/video-sidebar/`);
  });
  jQuery('.td_block_2 a').click(function () {
    link = $(this).attr('href');
    page = link.substr(link.lastIndexOf('/') + 1);
    ga('send', 'pageview', `/internal/must-read-sidebar/`);
  });
  // Sets up Captchas
  jQuery('#gd-footer-email').focusin(() => {
    jQuery('#newsletter-footer .g-recaptcha').show();
  });
  jQuery('#emailCC').focusin(() => {
    jQuery('#newsletter-concealed .g-recaptcha').show();
  });
  jQuery('#emailPop').focusin(() => {
    jQuery('#newsletter-popup .g-recaptcha').show();
  });
});
function onSubmit() {
  event.preventDefault();
  const postData = $('#newsletter-footer').serialize();
  jQuery.post('/wp-content/themes/gun-digest/reCaptchaProcessing.php', postData, (data) => {
    console.log(data);
    if (data == 'Success') {
      const email = document.getElementById('gd-footer-email').value;
      jQuery('#newsletter-footer').append(`<img src="https://app.bronto.com/public/?q=direct_add&fn=Public_DirectAddForm&id=bfdhrthklkecvxdpmmymgpuwtivhbbg&email=&createCookie=1&list4=0bd703ec00000000000000000000001df957" width="0" height="0" border="0"/>`);
      jQuery('.gd-footer-bar #success').show();
      jQuery('#gd-footer-email,#emailsubmit').hide();
    } else jQuery('.gd-footer-bar #error').show();
  });
  return false;
}
function onSubmitFree2() {
  event.preventDefault();
  const postData = jQuery('#free-download2').serialize();
  jQuery.post('/wp-content/themes/gun-digest/reCaptchaProcessing.php', postData, (data) => {
    if (data == 'Success') {
      document.getElementById('free-download2').submit();
    } else jQuery('.gd-footer-bar #error').show();
  });
  //
}
function onSubmitCC() {
  event.preventDefault();
  const postData = $('#newsletter-concealed').serialize();
  jQuery.post('/wp-content/themes/gun-digest/reCaptchaProcessing.php', postData, (data) => {
    if (data == 'Success') {
      document.getElementById('newsletter-concealed').submit();
    } else $('#newsletter-concealed #error').show();
  });
  //
}
function onSubmitPop() {
  event.preventDefault();
  const postData = $('#newsletter-popup').serialize();
  jQuery.post('/wp-content/themes/gun-digest/reCaptchaProcessing.php', postData, (data) => {
    if (data == 'Success') {
      document.getElementById('newsletter-popup').submit();
    } else $('#newsletter-popup #error').show();
  });
  //
}
function createCookie(name,value,days) {
    var expires = "";
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + value + expires + "; path=/";
}
function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}
function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}
