	<script>
    var isBodyLoaded;
    var isBackgroundPreloaded;
	document.body.className += " pearltrees-body  hide-rightcol  withRighColumnAds scrollers ";

	function onBackgroundPreloaded() {
		isBackgroundPreloaded = true;
		displayBackgroundIfNeeded();
	}
	
	function displayBackgroundIfNeeded() {
		if (isBodyLoaded && isBackgroundPreloaded) {
			var bg = document.getElementById('background1');
			var veil = document.getElementById('first-bg-veil');
			
			if (typeof bg.style['display'] !== "undefined") {
				bg.style.display = '';	
			}
			
			bg.style.filter = 'brightness(112%) blur(30px)';
			veil.style.backgroundColor = 'rgba(255, 255, 255, 0.7)';
			
			var hs = document.getElementById('horizontal-scroller');
			hs.removeChild(document.getElementById("backgroundPreloader"));
		}
	}
</script>
