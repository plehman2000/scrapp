function listenToComparisonTools(a){if("function"===typeof notifyComparisonTool){var b=notifyComparisonTool;notifyComparisonTool=function(c){b.call(this,c);a.call(this,c,"notifyComparisonTool")}}if("function"===typeof SlotComparisonTool){var d=SlotComparisonTool;SlotComparisonTool=function(c){d.call(this,c);a.call(this,c,"SlotComparisonTool")}}}
function onComparisonTool(a,b){ga4Item={};"notifyComparisonTool"===b?(ga4Item.event="add_brand_comparison",ga4ItemElement=a.closest("[data-operator]"),ga4Item.operator=ga4ItemElement.getAttribute("data-operator"),ga4Item.product=ga4ItemElement.getAttribute("data-product")):"SlotComparisonTool"===b&&(ga4Item.event="add_slot_comparison",ga4ItemElement=a.closest('[href*\x3d"/slot/"]'),ga4Item.slot_title=ga4ItemElement.getAttribute("title"));window.dataLayer=window.dataLayer||[];window.dataLayer.push(ga4Item)}
