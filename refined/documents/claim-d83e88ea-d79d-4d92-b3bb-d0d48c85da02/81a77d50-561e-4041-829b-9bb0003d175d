    function saveredirect(URL){
        var userToken = null  ; 
        if (window.localStorage) {
            userToken = window.localStorage.getItem('tpw.rToken');
        }
        if (userToken) {
            window.location = URL ;
        }else{
            window.location = '/user/join?redirect_url='+URL ;
        }                
    }

