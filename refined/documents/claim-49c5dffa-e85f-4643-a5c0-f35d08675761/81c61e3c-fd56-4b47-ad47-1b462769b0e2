function checkEmbedTop(element) {
  if (element[0].boundingClientRect.top > 0 && element[0].isIntersecting === false) {
    api.disableFloatingMode();
  } else if (element[0].isIntersecting === true) {
    api.enableFloatingMode();
  }
}


