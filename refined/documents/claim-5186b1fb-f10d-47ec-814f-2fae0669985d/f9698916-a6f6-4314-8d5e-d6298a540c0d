$(".picImg").click(function(){
	var imgUrl = $(this).attr('id');
	var thisImage = $(this).attr('src');
	if(thisImage == "https://files.abovetopsecret.com/images/show-img.png"){
		$(this).attr('src', imgUrl);
	}else{
		$(this).attr('src', 'https://files.abovetopsecret.com/images/show-img.png');
	}
});

