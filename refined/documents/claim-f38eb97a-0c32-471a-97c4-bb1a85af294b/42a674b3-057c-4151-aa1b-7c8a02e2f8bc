            window.addEventListener('DOMContentLoaded', function () {
                document.getElementById('loadMoreComments').onclick = function (e) {
                    var els = document.getElementsByClassName('comment-item--hidden'),
                        l = els.length;

                    while (l--) {
                        els[l].style.display = 'block';
                        els[l].classList.remove('comment-item--hidden');
                    }

                    window.dispatchEvent(new CustomEvent('refresh-comment-sections'));
                    e.target.style.display = 'none';
                };
            });
        </script>
