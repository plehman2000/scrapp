<script>
            jQuery(document).ready(function($) {
                $(document).on('click', '#trigger_cache_regen_button', function(e) {
                    e.preventDefault();

                    var postId = 13967;
                    var postType = 'post';

                    $.post(ajaxurl, {
                        action: 'cache_regen_action',
                        post_id: postId,
                        post_type: postType
