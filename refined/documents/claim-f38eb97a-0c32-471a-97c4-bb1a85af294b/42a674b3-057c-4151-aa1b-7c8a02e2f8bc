<path class="entry-info__bookmarkIcon" d="M6.8,10.5l4.9,3.3V1.9H1.9v11.9L6.8,10.5z M0,17.5V1.6C0,0.7,0.8,0,1.7,0h10.2
                                 c0.9,0,1.7,0.7,1.7,1.6v15.8l-6.8-4.6L0,17.5z"></path>
