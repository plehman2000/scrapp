        </div>
        <div class="sidebarDeals__meta">
            <a href="/deals/" class="btn btn-primary" target="_blank">See All Deals</a>
        </div>
    </script>
<script>
        function dcMove(fwd) {
            let el = document.getElementById("sidebarDealsCarousel"),
                curr = el.querySelector("li.active"),
                next = curr.nextElementSibling,
                prev = curr.previousElementSibling

            curr.classList.remove("active")
            if (fwd === true) {
                if (next) {
                    next.classList.add("active")
                } else {
                    el.querySelectorAll("li")[0].classList.add("active")
                }
            }

            if (fwd === false) {
                if (prev) {
                    prev.classList.add("active")
                } else {
                    el.querySelector("li:last-child").classList.add("active")
                }
            }
        }
    </script>
</div>
</aside>
</div>
</div>
</div>
