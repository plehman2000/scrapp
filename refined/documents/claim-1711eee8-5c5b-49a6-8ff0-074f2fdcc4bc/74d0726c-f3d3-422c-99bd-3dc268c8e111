 <script>
    function getEstimatedDeviceType(viewportWidth) {
        if (viewportWidth > 0 && viewportWidth <= 640) {
            return 'mobile';
        }
        if (viewportWidth > 640 && viewportWidth <= 1024) {
            return 'tablet';
        }
        if (viewportWidth > 1024) {
            return 'desktop';
        }
    }
</script>            <script async="" src="https://securepubads.g.doubleclick.net/tag/js/gpt.js" id="gpt-script"></script><script src="/build/./prebid/prebid.f0916f8e.js" async=""></script><script id="gpt-initializer">var googletag = googletag || {};googletag.cmd = googletag.cmd || [];</script>
