<script>function initComponent(domSelectors) {
    var $ = jQuery;
  //  General.initDefaultForm($(domSelectors.defaultForm, domSelectors.wrapper));
}

(function($){
    $('.essay-component input[autofocus]').each(function (index, item) {
        if ($(item).is(':visible')) {
            $(item).focus();
            return false;
        }
    });
})(jQuery);
