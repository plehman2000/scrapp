    <script type="text/javascript"><!-- Inline JavaScript for the Calculator -->
<script>
    document.addEventListener('DOMContentLoaded', function() {
        document.getElementById('calculateButton').addEventListener('click', function() {
            // Get values from the input fields
            let weight = parseFloat(document.getElementById('dogWeight').value);
            let activityFactor = parseFloat(document.getElementById('activityLevel').value);
            let caloriesPerCup = parseFloat(document.getElementById('caloriesPerCup').value);

            // Validate inputs
            if (isNaN(weight) || isNaN(caloriesPerCup) || isNaN(activityFactor)) {
                alert('Please fill in all the fields with valid numbers.');
                return;
            }

            // Calculate daily caloric requirement based on weight and activity level
            let dailyCalories = weight * activityFactor;

            // Calculate daily food amount in cups
            let dailyFoodAmount = dailyCalories / caloriesPerCup;

            // Output results
            document.getElementById('caloricResult').innerHTML = `Your dog's daily caloric requirement is approximately <strong>${Math.round(dailyCalories)} kcal</strong>.`;
            document.getElementById('foodAmountResult').innerHTML = `You should feed your dog approximately <strong>${dailyFoodAmount.toFixed(2)} cups</strong> of food per day.`;
        });
    });
</script>
